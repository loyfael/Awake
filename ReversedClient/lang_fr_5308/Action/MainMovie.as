DUNGEON_65_1 = "Sortez vainqueur du combat contre le Phorspiker !";
DUNGEON_65_2 = "Tu as battu le Phorspiker !";
DUNGEON_65_3 = "C\'est le moment de se donner à fond dans le combat. \n \nN ne s\'arrêtera pas avant d\'avoir détruit entièrement Emrub... il faut absolument le neutraliser !!";
DUNGEON_65_4 = ".";
DUNGEON_65_5 = "Tu te souviens désormais d\'une partie de l\'histoire d\'Emrub !";
