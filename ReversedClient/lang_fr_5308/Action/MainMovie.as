SPEAKING_SIGN_258_315_TITLE = "Boa";
SPEAKING_SIGN_263_254_CONTENT = "Bienvenue dans mon Havre ! \nJe pratique la méditation pour renforcer mes techniques de combat et me parfaire à l\'utilisation de mon Wakfu. \nJe ne fléchis pas et ne laisse en aucun cas mes sentiments prendre le dessus ! La maîtrise de soi est le maître mot !  \nMême en cas de catastrophe du nom de... Lock...";
SPEAKING_SIGN_263_254_TITLE = "Kouett";
