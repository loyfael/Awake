SPEAKING_SIGN_428_225_TITLE = ".";
SPEAKING_SIGN_428_UNDEFINED_CONTENT = ".";
SPEAKING_SIGN_428_UNDEFINED_TITLE = ".";
SPEAKING_SIGN_430_342_CONTENT = "Dites moi.. Vous savez quand vient Lock ? Il m\'a demandé via Fraise de l\'attendre au salon parce que c\'est « important » et depuis plus rien... Je ne le comprends vraiment pas...";
SPEAKING_SIGN_430_342_TITLE = "Mademoiselle Kouett";
SPEAKING_SIGN_435_342_CONTENT = "Il fait un peu chaud par ici quand même, je préfère Emrub! C\'est bien parce que je dois rendre visite à Blinie... \nSi tu es V.I.P, monte et on rentre ! c\'est plus sympa que de faire le chemin à pied non?";
SPEAKING_SIGN_435_342_TITLE = "Cyd";
SPEAKING_SIGN_456_194_CONTENT = "Hahaha ! Voui je viens jouer de suite avec vous !  \nAttendez moi ! Quelqu\'un a vu mon frère ?  \nOu est-il encore passé ... Yaze ! \n*Fixe froidement tes yeux*  \nQui es tu ?";
